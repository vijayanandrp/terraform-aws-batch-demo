{
  "command": [
    "ls",
    "-la"
  ],
  "image": "public.ecr.aws/runecast/busybox:1.33.1",
  "fargatePlatformConfiguration": {
    "platformVersion": "LATEST"
  },
  "resourceRequirements": [
    {
      "type": "VCPU",
      "value": "1"
    },
    {
      "type": "MEMORY",
      "value": "2048"
    }
  ],
  "executionRoleArn": "${aws_iam_role.ecs_task_execution_role.arn}"
}
